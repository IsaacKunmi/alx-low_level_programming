#include <stdio.h>
/**
 * main: Entry point
 * description: 'prints size of various types'
 * return: Always 0 (success)
 */

int main(void)
{
	printf("Size of a char: 1 byte(s)\nSize of an int: 4 byte(s)\nSize of a long int: 4 byte(s)\nSize of a long long int: 4 byte(s)\nSize of a float: 4 byte(s)");

	return (0);
}
